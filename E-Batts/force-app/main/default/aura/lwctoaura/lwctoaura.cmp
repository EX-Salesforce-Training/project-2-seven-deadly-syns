    <aura:attribute name="userVar1" type="string" default=''/>
    <aura:attribute name="acctVar1" type="string" default=''/>
    <aura:attribute name="qtyVar1" type="string" default='' />
    <aura:attribute name="dateVar1" type="string" default=''/>
    <aura:attribute name="prodVar1" type="string" default=''/>
    <aura:attribute name="pnameVar1" type="string" default=''/>
    <aura:attribute name="acctnameVar1" type="string" default=''/>
    <div class="slds-box slds-theme_default slds-theme_backgroundColor-lightblue">
   <lightning:layout>
        <lightning:layoutItem>
            <lightning:icon iconName="action:goal" alternativeText="My Items"/>
         </lightning:layoutItem >
        
         <lightning:layoutItem padding="horizontal-small">
             <div class="page-section page-header">
        <h1 class="slds-text-heading_label">Request a Quote for our Product </h1>
                 <!-- <span>
                <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/4/40/Nissan_Leaf_012.JPG/310px-Nissan_Leaf_012.JPG" width="300" height ="300"/>
                 </span>
                 <span>
                     <img src="https://www.edfenergy.com/sites/default/files/styles/webp__spire_tile__media__one_half__media_square/public/ev-electric-car-lithium-ion-battery.jpg?itok=Q8KaNaPZ" width="300" height="200"/>
                 </span>-->
                 </div>
        </lightning:layoutItem>
    </lightning:layout>
    
    
    <c:project2lwctoAura onproddata="{!c.handleProd}" onqtydata="{!c.handleQty}" ondatedata="{!c.handleDate}" onprodname="{!c.handleProdName}" onuserdata="{!c.handleUser}"  />
 
    
    <lightning:layout  class="slds-page-header slds-page-header_object-home">
        <lightning:layoutItem>
            <lightning:icon iconName="action:goal" alternativeText="My Products"/>
         </lightning:layoutItem >
        
         <lightning:layoutItem padding="horizontal-small">
             <div class="page-section page-header">
        <h1 class="slds-text-heading_label">Selected Product </h1>
                 </div>
        </lightning:layoutItem>
    </lightning:layout>
    
    <lightning:card footer="Account Manager will be notified "  title="{!v.acctnameVar1}">
       
        <aura:set attribute="actions">
            <lightning:button type="submit" variant="brand" class="slds-m-top_small" label="Submit" onclick="{!c.handleClick}"/>
        </aura:set>
        
        <p class="slds-p-horizontal_medium">
            Product Name:<ui:outputText value="{!v.pnameVar1}"/>
        </p>
        <p class="slds-p-horizontal_medium">
            Quantity:<ui:outputText value="{!v.qtyVar1}" />
        </p>
        <p class="slds-p-horizontal_medium">
            Quote Delivery Deadline:<ui:outputText value="{!v.dateVar1}" />
        </p>
    </lightning:card>
    </div>
</aura:component>